package impclasses;

public class I003_StringBufferBuilder {
	public static void main(String[] args) {
		
		
		StringBuffer sb = new StringBuffer("java");
		sb.append("Hello");
		System.out.println(sb);
		
		
		StringBuilder sb1 = new StringBuilder("java");
		sb1.append("Hello");
		System.out.println(sb1);
	}
}
