package array;

public class A006_MDArray {
	public static void main(String[] args) {
		
		
		
		int a[][] = new int[5][4];
		
		a[0][0] = 50;
		a[0][1] = 55;
		a[0][2] = 45;
		a[0][3] = 50;
		
		
		a[1][0] = 50;
		a[1][1] = 58;
		a[1][2] = 47;
		a[1][3] = 50;
		
		
		for(int i=0;i<5;i++)
		{
			for(int j=0;j<4;j++)
			{
				System.out.println(a[i][j]);
			}
		}
		
		
		
	}
}
